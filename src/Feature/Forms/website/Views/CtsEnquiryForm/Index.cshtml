@model CognizantPlc.Feature.Forms.Models.CustomerEnquiry
<h2>Enquiry Form</h2>

<form method="post">
    <div>
        Name:
        <input type="text" name="Name" id="Name" value="@Model.Name" />
    </div>
    <div>
        Email Id:
        <input type="text" name="EmailId" id="EmailId" value="@Model.EmailId" />
    </div>
    <div>
        Phone Number:
        <input type="text" name="PhoneNumber" id="PhoneNumber" value="@Model.PhoneNumber" />
    </div>
    <div>
        Enquiry:
        <input type="text" name="Enquiry" id="Enquiry" value="@Model.Enquiry" />
    </div>
    <input type="submit" value="Submit" />
</form>